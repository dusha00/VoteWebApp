@{
    Layout = null;
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<VoteWebApp.Models.Person>;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title></title>
    <meta content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0" name="viewport" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style" />
    <meta content="telephone=no" name="format-detection" />
    <link href="~/lib/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
</head>
<body>
    <section class="aui-flexView">
        <header class="aui-navBar aui-navBar-fixed b-line">
            <a href="javascript:;" class="aui-navBar-item">
                <i class="fa fa-arrow-left" onclick="GoBack()"></i>
            </a>
            <div class="aui-center">
                <span class="aui-center-title">Vote Meeting</span>
            </div>
        </header>
        <section class="aui-scrollView">
            <div class="aui-credit-list">
                @foreach (var person in Model)
                {
                    <a href="/Detail/Index" class="aui-flex">
                        <div class="aui-credit-img">
                            <img src="~/images/linux.png" alt="">
                        </div>
                        <div class="aui-flex-box">
                            <h1>linux</h1>
                            <p>Linux (/ˈlɪnəks/ (About this soundlisten) LIN-əks)[9][10] is a family of open source Unix-like operating systems based on the Linux kernel.</p>
                            <i class="fa fa-arrow-circle-right" style="color:white;"></i>
                        </div>
                    </a>
                }
            </div>
        </section>
    </section>
</body>

</html>

